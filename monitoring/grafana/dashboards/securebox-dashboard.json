{
  "id": null,
  "title": "SecureBox - File Sharing Platform",
  "tags": ["securebox"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total File Uploads",
      "type": "stat",
      "targets": [
        {
          "expr": "securebox_uploads_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Total File Downloads",
      "type": "stat",
      "targets": [
        {
          "expr": "securebox_downloads_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "Upload Rate (per 5min)",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(securebox_uploads_total[5m]) * 300",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
    },
    {
      "id": 4,
      "title": "Total Data Uploaded",
      "type": "stat",
      "targets": [
        {
          "expr": "securebox_file_size_bytes_sum",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
    },
    {
      "id": 5,
      "title": "Upload & Download Activity",
      "type": "timeseries",
      "targets": [
        {
          "expr": "increase(securebox_uploads_total[1m])",
          "refId": "A",
          "legendFormat": "Uploads per minute"
        },
        {
          "expr": "increase(securebox_downloads_total[1m])",
          "refId": "B", 
          "legendFormat": "Downloads per minute"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 6,
      "title": "File Size Distribution",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(securebox_file_size_bytes_sum[5m])",
          "refId": "A",
          "legendFormat": "Upload Rate (bytes/sec)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "binBps",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s",
  "schemaVersion": 39,
  "version": 1,
  "links": [],
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "hideControls": false,
  "liveNow": false,
  "style": "dark",
  "templating": {
    "list": []
  }
}
